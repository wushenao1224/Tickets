<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.bdqn.mapper.relatedImgMapper">
    <!--查询电影相关的图片；-->
    <select id="selectAllImg" resultType="cn.bdqn.domain.relatedImg">
        SELECT *
        FROM relatedimg
        WHERE filmID=#{filmID}
    </select>
</mapper>